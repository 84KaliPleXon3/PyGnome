<script>			
//secure data namespace
data = {};



//Gather and validate model data
function getgnomedata(){
	var speed = $("#speed").val();
	var direction = $("#direction").val();
	var latitude = $("#lat").val();
	var longitude = $("#long").val();
	var date = $("#datepicker").val();
	var time = $("#timepicker").val();
	
	//GNOME JSON object
	gnomedata = [
	{"speed": speed},
	{"direction": direction},
	{"latitude": latitude},
	{"longitude": longitude},
	{"date": time},
	{"time": date},
	]
	
	//GNOME JSON to be sent
	gnomedatajson = JSON.stringify(gnomedata);
}

//Client request in JSON
function sendgnomedata(){
jQuery.ajax({
type: 'POST',
url: 'http://localhost:6543/gnome',
data: gnomedatajson,
contentType: 'application/json; charset=utf-8'});
};

//Client test request in JSON
function jsontest(){

jQuery.ajax({
type: 'POST',
url: 'http://localhost:6543/run_spill',
data: gnomedatajson,
contentType: 'application/json; charset=utf-8'});
};


//Adds map button in jquery
function addmap()
{
var n = 0;
if(n!=1){
$('#navi').append('<button id="showspillbutton" onclick="addspillmap();">Show Spill</button>');
}
n=1;
}

//Add the static spill map
function addspillmap()
{
$("#mappng").attr("src","static/ppng.png");
$('#navi').append('<button id="showmoviebutton" onclick="movie()">Simulate</button>');
simulate();
}

//Add the dynamic spill map---Cacher-----
function simulate()
{
//load the simulation
